local Kaitun = Main:CreateTab("Kaitun",false)
Kaitun:CreateToggle("Kaitun",false,function()
	_G.FarmLevel = true
	_G.AutoNew = true
	_G.AutoThird = true
	_G.Superhuman = true
	_G.Stopanime = true
end)
spawn(function()
    local gg = getrawmetatable(game)
    local old = gg.__namecall
    setreadonly(gg,false)
    gg.__namecall = newcclosure(function(...)
        local method = getnamecallmethod()
        local args = {...}
        if tostring(method) == "FireServer" then
            if tostring(args[1]) == "RigControllerEvent" then
                args[4] = 3 
                return old(unpack(args))
            end
        end
        return old(...)
    end)
end)


--ซ่อน UI
spawn(function()
    while task.wait() do
        if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.RightControl) then
            if game:GetService("CoreGui").Sawadd.Enabled == false then
                game:GetService("CoreGui").Sawadd.Enabled = true
                wait(.1)
            elseif game:GetService("CoreGui").Sawadd.Enabled == true then
                game:GetService("CoreGui").Sawadd.Enabled = false
                wait(.1)
            end
        end
    end
end)

return UILib
