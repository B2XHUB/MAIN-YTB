		while getgenv().A do 
			wait(.1)
	
		local name = game.Players.LocalPlayer
		_G.name = name
		local namex = tostring(_G.name)
	   for i,v in pairs(game:GetService("Workspace").Characters[namex].HumanoidRootPart:GetChildren()) do
		if v.Name == "CrewBBG" then   
		 v:Destroy()
		end
	   end
	   for i,v in pairs(game:GetService("Workspace").Characters[namex].HumanoidRootPart:GetChildren()) do
