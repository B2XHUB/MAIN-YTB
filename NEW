			UICorner_6.CornerRadius = UDim.new(0, 3)
			UICorner_6.Parent = ToggleFrameIn

			check.Name = "check"
			check.Parent = ToggleFrameIn
			check.BackgroundColor3 = Color3.fromRGB(1, 27, 0)
			check.LayoutOrder = 1
			check.Position = UDim2.new(0, 2, 0, 2)
